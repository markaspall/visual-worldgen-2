{
  "name": "visual-world-gen",
  "version": "2.0.0",
  "description": "Browser-based procedural map generation system with WebGPU and server-side chunk generation",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "start:test_world": "cross-env WORLD_ID=test_world node server.js",
    "dev:test_world": "cross-env WORLD_ID=test_world node --watch server.js",
    "start:my_world": "cross-env WORLD_ID=my_world PORT=3013 node server.js",
    "start:production": "cross-env WORLD_ID=production PORT=3014 node server.js",
    "monitor": "node server/monitorServer.js",
    "test:gpu": "node server/test-gpu.js"
  },
  "keywords": ["webgpu", "procedural", "map-generation", "node-editor", "svdag", "voxel"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "webgpu": "latest"
  }
}
